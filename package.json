{
  "version": "1.2.5",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "lint-fix": "tsdx lint --fix",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepublishOnly": "yarn test && yarn build",
    "postversion": "npm publish",
    "postpublish": "git push && git push --tags"
  },
  "peerDependencies": {
    "react": ">=16",
    "stylis": ">=4",
    "@types/stylis": ">=4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "atyled",
  "author": "Behnam Mohammadi",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atyled/atyled.git"
  },
  "bugs": {
    "url": "https://github.com/atyled/atyled.git/issues"
  },
  "homepage": "https://github.com/atyled/atyled.git#readme",
  "module": "dist/atyled.esm.js",
  "size-limit": [
    {
      "path": "dist/atyled.cjs.production.min.js",
      "limit": "2.5 KB"
    },
    {
      "path": "dist/atyled.esm.js",
      "limit": "2.5 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@size-limit/preset-small-lib": "^8.2.4",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addons": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/styled-components": "^5.1.26",
    "babel-loader": "^9.1.2",
    "husky": "^8.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "size-limit": "^8.2.4",
    "styled-components": "^5.3.6",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "keywords": [
    "atyled",
    "atomic css",
    "styled-components",
    "react"
  ],
  "dependencies": {
    "@types/stylis": "^4.0.2",
    "stylis": "^4.1.3"
  },
  "resolutions": {
    "**/@typescript-eslint/eslint-plugin": "^4.1.1",
    "**/@typescript-eslint/parser": "^4.1.1"
  }
}
